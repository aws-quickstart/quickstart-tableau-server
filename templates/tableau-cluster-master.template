                    },
                    "RegCompany": {
                        "Ref": "RegCompany"
                    },
                    "RegCountry": {
                        "Ref": "RegCountry"
                    },
                    "RegDepartment": {
                        "Ref": "RegDepartment"
                    },
                    "RegEmail": {
                        "Ref": "RegEmail"
                    },
                    "RegFirstName": {
                        "Ref": "RegFirstName"
                    },
                    "RegIndustry": {
                        "Ref": "RegIndustry"
                    },
                    "RegLastName": {
                        "Ref": "RegLastName"
                    },
                    "RegPhone": {
                        "Ref": "RegPhone"
                    },
                    "RegState": {
                        "Ref": "RegState"
                    },
                    "RegTitle": {
                        "Ref": "RegTitle"
                    },
                    "RegZip": {
                        "Ref": "RegZip"
                    },
                    "TableauServerLicenseKey": {
                        "Ref": "TableauServerLicenseKey"
                    },
                    "TableauServerPrimaryInstaller": {
                        "Fn::FindInMap": [
                            "DefaultConfiguration",
                            "InstallationConfig",
                            "InstallationExecutable"
                        ]
                    },
                    "TableauServerWorkerInstaller": {
                        "Fn::FindInMap": [
                            "DefaultConfiguration",
                            "InstallationConfig",
                            "InstallationWorkerExecutable"
                        ]
                    },
                    "VpcId": {
                        "Fn::GetAtt": [
                            "VPCStack",
                            "Outputs.VPCID"
                        ]
                    },
                    "WindowsAMIId": {
                        "Fn::FindInMap": [
                            "AWSAMIRegionMap",
                            {
                                "Ref": "AWS::Region"
                            },
                            {
                                "Fn::FindInMap": [
                                    "DefaultConfiguration",
                                    "MachineConfiguration",
                                    "WindowsVersion"
                                ]
                            }
                        ]
                    }
                }
            }
        },
        "LoadBalancerStack": {
            "Type": "AWS::CloudFormation::Stack",
            "DependsOn": [
                "VPCStack"
            ],
            "Properties": {
                "TemplateURL": {
                    "Fn::Join": [
                        "/",
                        [
                            {
                                "Fn::FindInMap": [
                                    "AWSInfoRegionMap",
                                    {
                                        "Ref": "AWS::Region"
                                    },
                                    "QuickStartS3URL"
                                ]
                            },
                            {
                                "Ref": "QSS3BucketName"
                            },
                            {
                                "Ref": "QSS3KeyPrefix"
                            },
                            "templates/loadbalancer.template"
                        ]
                    ]
                },
                "Parameters": {
                    "LoadBalancerPublicSubnets": {
                        "Fn::Join": [
                            ",",
                            [
                                {
                                    "Fn::GetAtt": [
                                        "VPCStack",
                                        "Outputs.PublicSubnet1ID"
                                    ]
                                },
                                {
                                    "Fn::GetAtt": [
                                        "VPCStack",
                                        "Outputs.PublicSubnet2ID"
                                    ]
                                },
                                {
                                    "Fn::GetAtt": [
                                        "VPCStack",
                                        "Outputs.PublicSubnet3ID"
                                    ]
                                }
                            ]
                        ]
                    },
                    "SourceCidr": {
                        "Ref": "SourceCidr"
                    },
                    "SSLCertificateARN": {
                        "Ref": "SSLCertificateARN"
                    },
                    "VpcId": {
                        "Fn::GetAtt": [
                            "VPCStack",
                            "Outputs.VPCID"
                        ]
                    }
                }
            }
        },
        "WorkloadSecurityGroupLoadBalancerIngress": {
            "Type": "AWS::EC2::SecurityGroupIngress",
            "DependsOn": [
                "LoadBalancerStack",
                "WorkloadStack"
            ],
            "Properties": {
                "GroupId": {
                    "Fn::GetAtt": [
                        "WorkloadStack",
                        "Outputs.SecurityGroup"
                    ]
                },
                "IpProtocol": "-1",
                "SourceSecurityGroupId": {
                    "Fn::GetAtt": [
                        "LoadBalancerStack",
                        "Outputs.SecurityGroup"
                    ]
                }
            }
        },
        "WorkloadSecurityGroupBastionRDPIngress": {
            "Type": "AWS::EC2::SecurityGroupIngress",
