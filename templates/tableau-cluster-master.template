                            "DefaultConfiguration",
                            "NetworkConfiguration",
                            "Worker1Ip"
                        ]
                    },
                    "Worker1SubnetId": {
                        "Fn::GetAtt": [
                            "VPCStack",
                            "Outputs.PrivateSubnet2AID"
                        ]
                    },
                    "Worker2AvailabilityZone": {
                        "Fn::Select": [
                            "2",
                            {
                                "Ref": "AvailabilityZones"
                            }
                        ]
                    },
                    "Worker2Ip": {
                        "Fn::FindInMap": [
                            "DefaultConfiguration",
                            "NetworkConfiguration",
                            "Worker2Ip"
                        ]
                    },
                    "Worker2SubnetId": {
                        "Fn::GetAtt": [
                            "VPCStack",
                            "Outputs.PrivateSubnet3AID"
